{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "centerTitle": true,
        "backgroundColor": "#f4511e"
      }
    },
    "body": {
      "type": "scaffold",
      "args": {
        "appBar": {
          "type": "app_bar",
          "args": {
            "title": {
              "type": "text",
              "args": {
                "text": "Login",
                "style": {
                  "color": "#000000",
                  "fontWeight": "bold"
                }
              }
            },
            "centerTitle": true,
            "backgroundColor": "#ffffff",
            "elevation": 5
          }
        },
        "backgroundColor": "#ffffff",
        "body": {
          "type": "safe_area",
          "args": {
            "bottom": true,
            "child": {
              "type": "form",
              "args": {
                "child": {
                  "type": "single_child_scroll_view",
                  "args": {
                    "padding": [
                      16,
                      0
                    ],
                    "child": {
                      "type": "container",
                      "args": {
                        "alignment": "topCenter",
                        "width": "infinity",
                        "child": {
                          "type": "container",
                          "args": {
                            "constraints": {
                              "maxWidth": 450
                            },
                            "height": "MediaQuery.of(context).size.height",
                            "width": "MediaQuery.of(context).size.width",
                            "child": {
                              "type": "column",
                              "args": {
                                "mainAxisSize": "min",
                                "children": [
                                  {
                                    "type": "sized_box",
                                    "args": {
                                      "height": 16
                                    }
                                  },
                                  {
                                    "type": "material",
                                    "args": {
                                      "color": "#FFFFFF",
                                      "elevation": 4,
                                      "margin": [
                                        0,
                                        8
                                      ],
                                      "padding": 16,
                                      "child": {
                                        "type": "column",
                                        "args": {
                                          "mainAxisSize": "min",
                                          "children": [
                                            {
                                              "type": "text_form_field",
                                              "id": "MobileNumber",
                                              "args": {
                                                "decoration": {
                                                  "labelText": "Mobile Number",
                                                  "hintText": "1234567"
                                                },
                                                "keyboardType": "number",
                                                "validators": [
                                                  {
                                                    "type": "required"
                                                  },
                                                  {
                                                    "length": 7,
                                                    "type": "max_length"
                                                  },
                                                  {
                                                    "length": 7,
                                                    "type": "min_length"
                                                  }
                                                ]
                                              }
                                            },
                                            {
                                              "type": "sized_box",
                                              "args": {
                                                "height": 16
                                              }
                                            },
                                            {
                                              "type": "text_form_field",
                                              "id": "Pin",
                                              "args": {
                                                "keyboardType": "number",
                                                "obscureText": "${ObscureText}",
                                                "isPin": "true",
                                                "decoration": {
                                                  "hintText": "1234",
                                                  "labelText": "Pin",
                                                  "suffixIcon": {
                                                    "type": "icon_button",
                                                    "args": {
                                                      "icon": {
                                                        "type": "icon",
                                                        "args": {
                                                          "icon": {
                                                            "codePoint": "0xe6be",
                                                            "fontFamily": "MaterialIcons",
                                                            "size": 50
                                                          }
                                                        }
                                                      },
                                                      "onPressed": "${set_value('ObscureText', '!ObscureText')}"
                                                    }
                                                  }
                                                },
                                                "validators": [
                                                  {
                                                    "type": "required"
                                                  },
                                                  {
                                                    "length": 4,
                                                    "type": "max_length"
                                                  },
                                                  {
                                                    "length": 4,
                                                    "type": "min_length"
                                                  }
                                                ]
                                              }
                                            }
                                          ]
                                        }
                                      }
                                    }
                                  },
                                  {
                                    "type": "sized_box",
                                    "args": {
                                      "height": 16
                                    }
                                  },
                                  {
                                    "type": "elevated_button",
                                    "id": "submit_button",
                                    "args": {
                                      "onPressed": "${validateForm('form_context')}",
                                      "child": {
                                        "type": "container",
                                        "args": {
                                          "alignment": "center",
                                          "width": "infinity",
                                          "child": {
                                            "type": "save_context",
                                            "args": {
                                              "key": "form_context",
                                              "child": {
                                                "type": "text",
                                                "args": {
                                                  "text": "Login",
                                                  "style": {
                                                    "color": "#ffffff",
                                                    "fontWeight": "bold",
                                                    "fontSize": 20
                                                  }
                                                }
                                              }
                                            }
                                          }
                                        }
                                      }
                                    }
                                  },
                                  {
                                    "type": "sized_box",
                                    "args": {
                                      "height": 30
                                    }
                                  },
                                  {
                                    "type": "container",
                                    "args": {
                                      "decoration": {
                                        "border": {
                                          "color": "#838383"
                                        }
                                      },
                                      "height": 35,
                                      "child": {
                                        "type": "elevated_button",
                                        "id": "New_User",
                                        "args": {
                                          "style": {
                                            "backgroundColor": "#FFFFFF",
                                            "foregroundColor": {
                                              "pressed": "#000000",
                                              "focused": "#000000",
                                              "empty": "#000000"
                                            }
                                          },
                                          "onPressed": "${RegisterUser()}",
                                          "child": {
                                            "type": "container",
                                            "args": {
                                              "alignment": "center",
                                              "width": "infinity",
                                              "child": {
                                                "type": "save_context",
                                                "args": {
                                                  "key": "form_context",
                                                  "child": {
                                                    "type": "text",
                                                    "args": {
                                                      "text": "New Wallet",
                                                      "style": {
                                                        "color": "#000000",
                                                        "fontWeight": "bold",
                                                        "fontSize": 20
                                                      }
                                                    }
                                                  }
                                                }
                                              }
                                            }
                                          }
                                        }
                                      }
                                    }
                                  }
                                ]
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}

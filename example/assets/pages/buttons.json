{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "Buttons"
          }
        }
      }
    },
    "body": {
      "type": "column",
      "children": [
        {
          "type": "expanded",
          "child": {
            "type": "center",
            "child": {
              "type": "elevated_button",
              "args": {
                "onLongPress": "##simplePrintMessage()##",
                "style": {
                  "foregroundColor": "##materialCallback()##"
                }
              },
              "child": {
                "type": "text",
                "args": {
                  "text": "ElevatedButton"
                }
              }
            }
          }
        },
        {
          "type": "expanded",
          "child": {
            "type": "center",
            "child": {
              "type": "text_button",
              "args": {
                "onLongPress": "##simplePrintMessage()##",
                "style": {
                  "foregroundColor": "##materialCallback()##"
                }
              },
              "child": {
                "type": "text",
                "args": {
                  "text": "TextButton"
                }
              }
            }
          }
        }
      ]
    }
  }
}